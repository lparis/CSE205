package module6;

// Prints first n Fibonacci numbers

public class QT 
{
	public static void main(String[] args) 
	{
		fun(11);
	}
	
	public static void fun(int n)
	{
		Queue q = new Queue();
		q.enqueue(0);
		q.enqueue(1);
		
		for (int i = 0; i < n; i++) 
		{
			int a = q.dequeue();
			int b = q.dequeue();
			q.enqueue(b);
			q.enqueue(a + b);
			System.out.println(a + " ");
		}
	}  
}
