package module6;

import java.util.Scanner;

public class Fibonacci 
{	
	public static void main(String[] args) 
	{
		Scanner in = new Scanner(System.in);
		System.out.print("Enter a Fibonacci: ");
		int num = in.nextInt();
		while (num > 48)
			System.out.println("Enter a number less than 48");
		fib(num);
//		if (num < 48)
//			fib(num);
//		else
//		{
//			System.out.print("Enter a number less than 48");
//		}
//		in.close();
	}
	
	public static void fib(int n)
	{
		Queue q = new Queue();
		q.enqueue(0);
		q.enqueue(1);
		
		for (int i = 0; i < n; i++) 
		{
			int a = q.dequeue();
			int b = q.dequeue();
			q.enqueue(b);
			q.enqueue(a + b);
			System.out.println(a + "");
		}
	}  
}
